{
	"$schema": "https://turbo.build/schema.json",
	"ui": "tui",
	"tasks": {
		"build": {
			"dependsOn": ["^build"],
			"outputs": ["dist/**", ".next/**", ".astro/**"]
		},
		"dev": {
			"cache": false,
			"persistent": true
		},
		"lint": {},
		"format": {},
		"check": {},
		"test": {
			"dependsOn": ["build"]
		},
		"clean": {
			"cache": false
		}
	}
}
