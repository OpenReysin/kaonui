{
	"name": "@kaonui/vue",
	"version": "0.1.0",
	"description": "Vue wrappers for kaonui Lit components",
	"type": "module",
	"main": "./dist/index.cjs.js",
	"module": "./dist/index.es.js",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.es.js",
			"require": "./dist/index.cjs.js"
		}
	},
	"files": [
		"dist",
		"README.md"
	],
	"scripts": {
		"build": "vite build",
		"dev": "vite build --watch",
		"lint": "biome lint src/",
		"format": "biome format --write src/",
		"check": "biome check src/",
		"type-check": "vue-tsc --noEmit",
		"test": "vitest",
		"clean": "rm -rf dist",
		"prepublishOnly": "npm run build"
	},
	"dependencies": {
		"@kaonui/lit": "workspace:*"
	},
	"devDependencies": {
		"@biomejs/biome": "^1.4.1",
		"@microsoft/api-extractor": "^7.52.13",
		"@vitejs/plugin-vue": "^6.0.1",
		"@vue/tsconfig": "^0.8.1",
		"typescript": "~5.8.3",
		"unplugin-dts": "^1.0.0-beta.0",
		"vite": "^7.1.6",
		"vitest": "^0.34.6",
		"vue": "^3.3.4",
		"vue-tsc": "^3.0.7"
	},
	"peerDependencies": {
		"vue": "^3.3.0"
	},
	"engines": {
		"node": ">=18.0.0"
	},
	"repository": {
		"type": "git",
		"url": "your-repo-url"
	},
	"keywords": [
		"vue",
		"components",
		"ui",
		"lit",
		"web-components"
	],
	"author": "Your Name",
	"license": "MIT"
}
